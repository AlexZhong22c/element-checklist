  "description": "a tool for starting up an element-ui component",
  "bin": {
    "e-checklist": "./bin/index.js"
  },
  "keywords": [
    "vue",
    "element-ui",
    "vue-element-admin"
  ],
  "bugs": {
    "url": "https://github.com/xxxxxx/element-checklist/issues"
